{
  "env": {
    "mocha": true
  },
  "globals": {
    "appendToBody": false,
    "ariaExtensions": false,
    "cleanDom": false,
    "domUtils": false,
    "expect": false,
    "ExtendedArray": false,
    "findSymbol": false,
    "mock": false,
    "symbols": false,
    "uniqueId": false
  },
  "rules": {
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "one-var": 0,
    "one-var-declaration-per-line": 0,
    "class-methods-use-this": 0
  }
}
